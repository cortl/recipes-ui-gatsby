  "description": "Gatsby website for displaying cortl's recipes",
  "version": "0.1.0",
  "author": "Cortlan Bainbridge <hello@cortlan.dev>",
  "dependencies": {
    "@cortl/recipes": "^2.0.4",
    "fuse.js": "^3.4.6",
    "gatsby": "^2.18.12",
