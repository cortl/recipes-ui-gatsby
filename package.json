{
  "name": "recipes-ui",
  "private": true,
  "description": "Gatsby website for displaying cortl's recipes",
  "version": "0.1.0",
  "author": "Cortlan Bainbridge <hello@cortlan.dev>",
  "dependencies": {
    "@cortl/recipes": "^v2.1.175",
    "fuse.js": "^6.4.6",
    "gatsby": "^3.14.3",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-sass": "^4.14.0",
    "gatsby-plugin-sharp": "^3.14.1",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-json": "^3.14.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "node-sass": "^6.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "tachyons-sass": "^4.9.5"
  },
  "devDependencies": {
    "firebase-tools": "^9.20.0",
    "husky": "^7.0.2",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1"
  },
  "keywords": [
    "gatsby"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern \"src/**/*.js\""
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cortl/recipes-ui"
  },
  "bugs": {
    "url": "https://github.com/cortl/recipes-ui/issues"
  }
}
